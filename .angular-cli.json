{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
      "name": "techBlog"
    },
    "apps": [
      {
        "root": "src",
        "outDir": "dist",
        "assets": [
          "assets",
          "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
          "styles.css",
          "assets/css/bootstrap.css",
          "assets/css/bootstrap-responsive.css",
          "assets/css/animate.css",
          "assets/css/style.css",
          "assets/color/default.css"
        ],
        "scripts": [
          "assets/js/jquery.min.js",
          "assets/js/jquery.easing.js",
          "assets/js/bootstrap.js",
          "assets/js/parallax/jquery.parallax-1.1.3.js",
          "assets/js/nagging-menu.js",
          "assets/js/jquery.nav.js",
          "assets/js/prettyPhoto/jquery.prettyPhoto.js",
          "assets/js/portfolio/jquery.quicksand.js",
          "assets/js/portfolio/setting.js",
          "assets/js/hover/jquery-hover-effect.js",
          "assets/js/jquery.scrollTo.min.js",
          "assets/js/animate.js",
          "assets/js/contactform.js",
          "assets/js/custom.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
          "dev": "environments/environment.ts",
          "prod": "environments/environment.prod.ts"
        }
      }
    ],
    "e2e": {
      "protractor": {
        "config": "./protractor.conf.js"
      }
    },
    "lint": [
      {
        "project": "src/tsconfig.app.json"
      },
      {
        "project": "src/tsconfig.spec.json"
      },
      {
        "project": "e2e/tsconfig.e2e.json"
      }
    ],
    "test": {
      "karma": {
        "config": "./karma.conf.js"
      }
    },
    "defaults": {
      "styleExt": "css",
      "component": {}
    }
  }